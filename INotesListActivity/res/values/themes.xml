    <style name="Theme.INotes.Light.CustomActionBar" parent="Theme.INotes.Light.Default">
        <!--ToolBar颜色-->
        <item name="colorPrimary">@color/toolbar_yellow</item>
        <!--状态栏颜色-->
        <item name="colorPrimaryDark">@color/statubar_yellow</item>
        <!--输入框焦点、checkbox选中等颜色-->
        <item name="colorAccent">@color/gray_button</item>

        <item name="actionModeBackground">@color/toolbar_yellow</item>

        <item name="android:textColorSecondary">@color/text_black</item>

        <item name="android:windowBackground">@color/white</item>

    </style>

    <style name="Theme.INotes.Dark.CustomActionBar" parent="Theme.INotes.Dark.Default">
        <!--ToolBar颜色-->
        <item name="colorPrimary">@color/text_black</item>
        <!--状态栏颜色-->
        <item name="colorPrimaryDark">@color/text_black</item>
        <!--输入框焦点、checkbox选中等颜色-->
        <item name="colorAccent">@color/toolbar_yellow</item>
        <!--ActionMode背景色-->
        <item name="actionModeBackground">@color/text_black</item>
        <!--Toolbar图标颜色\系统菜单键列表项字体颜色\pref列表项summary字体颜色-->
        <item name="android:textColorSecondary">@color/accent</item>

        <item name="android:windowBackground">@color/dark_background</item>

    </style>

